{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"WvW Overlay",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"SirNikkolas",
		"icon":"castle.red.png",
		"icon_gray":"castle.png",
		"description":"WvW App"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":400, "height":300},
					"min_size":{"width":200, "height":200},
					"max_size":{"width":600, "height":500}
				},
			"SubWindow":{
					"file":"Files/subwindow.html",
					"transparent":false,
					"resizable":true,
					"size":{"width":400, "height":300},
					"in_game_only":true
				},
			"WvWMapWindow":{
					"file":"Files/WvWMapWindow.html",
					"transparent":true,
					"resizable":false,
					"size":{"width":681, "height":877},
					"in_game_only":false
				}
		}
	}
}